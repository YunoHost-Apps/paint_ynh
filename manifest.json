{
    "name": "Paint",
    "id": "paint",
    "packaging_format": 1,
    "description": {
        "en": "Web Components-based remake of MS Paint using modern web capabilities.",
        "fr": "Remake basé sur des composants Web de MS Paint utilisant des fonctionnalités Web modernes."
    },
    "version": "1.0~ynh1",
    "url": "https://github.com/christianliebel/paint",
    "upstream": {
        "license": "AGPL-2.0",
        "website": "https://paint.js.org/",
        "demo": "https://paint.js.org/",
        "userdoc": "https://github.com/christianliebel/paint",
        "code": "https://github.com/christianliebel/paint"
    },
    "license": "AGPL-2.0",
    "maintainer": {
        "name": "Jarod5001",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/paint",
                "default": "/paint"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
